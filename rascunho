#include <stdio.h>

int fib(int x) {   //sequencia de fibonacci
    if (x == 0) {
        return 0;
    }
    if (x == 1) {
        return 1;
    }
    
    int fib[x + 1];
    fib[0] = 0;
    fib[1] = 1;
    for (int i = 2; i <= x; i++) {
        fib[i] = fib[i - 1] + fib[i - 2];
    }
    return fib[x];
}





        
