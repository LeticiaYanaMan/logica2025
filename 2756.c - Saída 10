#include <stdio.h>
 
int main() {
    char A = 'A';
    printf("       A\n");
    char B = 'B';
    printf("      B B\n");
    char C = 'C';
    printf("     C   C\n");
    char D = 'D';
    printf("    D     D\n");
    char E = 'E';
    printf("   E       E\n");
    printf("    D     D\n");
    printf("     C   C\n");
    printf("      B B\n");
    printf("       A\n");
    
    return 0;
}
