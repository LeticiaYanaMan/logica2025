#include <stdio.h>

int main() {
  int H, Z, L;
  scanf("%d %d %d", &H, &Z, &L);
  if (H < Z && Z < L) {
    printf("zezinho\n");
  }
  else if (L < Z && Z < H) {
    printf("zezinho\n"); 
  }
  else if (Z < H && H < L) {
    printf("huguinho\n");
  }
  else if (L < H && H < Z) {
    printf("huguinho\n");
  }
  else if (H < L && L < Z) {
    printf("luisinho\n");
  }
  else if (Z < L && L < H) {
    printf("luisinho\n);
  }
  return 0;
}
